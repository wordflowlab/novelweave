{
	"name": "@novelweave/cli",
	"version": "0.0.2-alpha.2",
	"description": "Terminal User Interface for NovelWeave",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"novelweave": "dist/index.js",
		"nw": "dist/index.js"
	},
	"scripts": {
		"build": "node esbuild.config.mjs",
		"dev": "pnpm build --watch",
		"deps:install": "npm install --prefix ./dist",
		"start": "node dist/index.js",
		"clean": "rimraf dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"check-types": "tsc --noEmit",
		"format": "prettier --write src",
		"logs": "clear && tail -f ~/.novelweave/cli/logs/cli.txt",
		"logs:clear": "rimraf ~/.novelweave/cli/logs/* && touch ~/.novelweave/cli/logs/cli.txt",
		"clean:novelweave": "npx del-cli ./dist/novelweave --force && npx mkdirp ./dist/novelweave",
		"copy:novelweave": "npx cpy '../bin-unpacked/extension/**' './dist/novelweave' --parents",
		"lint": "eslint ."
	},
